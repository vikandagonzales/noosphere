    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass src/stylesheets/sass/main.scss src/stylesheets/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output src/stylesheets/css/main.css src/stylesheets/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch | react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
