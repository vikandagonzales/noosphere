  "dependencies": {
    "autoprefixer": "^9.5.0",
    "axios": "^0.18.0",
    "bulma": "^0.7.4",
    "bulma-extensions": "^6.2.3",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.4.0",
    "react-scripts": "2.1.8",
